{
  "name": "envision/craft-site-navigation",
  "description": "Custom navigation manager.",
  "version": "1.0.1",
  "type": "craft-plugin",
  "keywords": ["navigation"],
  "license": "MIT",
  "authors": [
    {
      "name": "Envision",
      "homepage": "https://www.envisionsuccess.net"
    }
  ],
  "require": {
    "craftcms/cms": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "envision\\sitenavigation\\": "src/"
    }
  },
  "extra": {
    "handle": "site-navigation",
    "name": "Site Navigation",
    "hasSettings": "true",
    "hasCpSection": "true",
    "class": "envision\\sitenavigation\\SiteNavigation"
  }
}
